set size 1, 1
set origin 0,0


set autoscale
unset log
unset label
unset title
set xtic auto
set ytic auto

set xlabel "Dp (nm)"
set ylabel "Number (1/cc)"

set xrange [10:1000]
set yrange [0:140000]

set logscale x

# Number
set title "7/28/2014"
set xlabel "Dp (nm)"
set ylabel "dN/dlogDp (cm-3)"
plot "7_28_2014_obs_num_size_dist.txt" using 1:5 title 'observed' with points 1, \
     "soa.aer.txt" using 2:3 title 'model' with lines 1


