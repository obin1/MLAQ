set size 1, 1
set origin 0,0


set autoscale
unset log
unset label
unset title
set xtic auto
set ytic auto

set xrange [0:5]

# Number
set title "7/30/2014"
set xlabel "Time (min)"
set ylabel "isoprene (ppbv)"
plot "isoprene_decay_7_30.txt" using 1:2 title 'observed isoprene' with points 1, \
     "soa_formation_7_30.txt" using 1:3 title 'observed SOA' with points 2, \
     "soa.gas.txt" using 1:2 title 'model isoprene' with lines 1, \
     "soa.gas.txt" using 1:3 title 'model SOA' with lines 2


